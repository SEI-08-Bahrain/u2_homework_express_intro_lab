<!DOCTYPE html>
<html>
<head>
    <title>Menu Items</title>
</head>
<body>
    <h1>Menu Items</h1>
    <ul>
        <% menuItems.forEach(item => { %> 
            <li>
              <%= item.name %>
              -
              <%= item.price %>
            </li>
        <% }); %>
    </ul>
</body>
</html>
